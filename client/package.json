      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "autoprefixer": "^9.6.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "tailwindcss": "^1.4.6"
  }
